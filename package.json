{
    "name": "study-online",
    "private": true,
    "description": "Beauty",
    "devDependencies": {
        "gulp": "^4.0.2",
        "gulp-clean": "^0.4.0",
        "gulp-dart-sass": "^1.1.0",
        "gulp-plumber": "^1.2.1",
        "gulp-postcss": "^9.0.1",
        "postcss": "^8.4.29",
        "prettier": "^3.0.3",
        "sass": "^1.67.0"
